{
    "name": "novaboltlabs/cartify",
    "type": "project",
    "description": "Laravel ecommerce platform",
    "keywords": [
        "laravel",
        "livewire",
        "alpinejs",
        "tailwindcss",
        "cartify",
        "ecommerce"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "akaunting/laravel-money": "^4.0",
        "artesaos/seotools": "^1.2",
        "blade-ui-kit/blade-heroicons": "^2.1",
        "cybercog/laravel-ban": "^4.8",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.12",
        "livewire/livewire": "^2.11",
        "propaganistas/laravel-phone": "^5.0",
        "razorpay/razorpay": "^2.8",
        "spatie/laravel-livewire-wizard": "^1.2",
        "spatie/laravel-medialibrary": "^10.7",
        "spatie/laravel-settings": "^3.0",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/laravel-stripe-webhooks": "^3.2",
        "spatie/laravel-webhook-client": "^3.1",
        "staudenmeir/laravel-adjacency-list": "^1.13",
        "stripe/stripe-php": "^10.5",
        "ublabs/blade-simple-icons": "^0.71.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.19",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
